{
  "name": "telar-social-engine",
  "version": "0.7.0",
  "description": "Telar cloud social engine",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "craco start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "craco build",
    "postbuild": "rm -rf ts-frontend/dist && cp -R build ts-frontend/dist",
    "build": "GENERATE_SOURCEMAP=false npm-run-all build-css build-js",
    "test": "NODE_ENV=test craco test --env=jsdom --modulePaths=src --moduleNameMapper='{\"^src/(.*)$\": \"<rootDir>/src/$1\", \"^models/(.*)$\": \"<rootDir>/src/models/$1\", \"^locales/(.*)$\": \"<rootDir>/src/locales/$1\", \"^config$\": \"<rootDir>/src/config/index\"}'",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "format": "prettier --write src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "home-page": "node -e \"let pkg=require('./package.json'); pkg.homepage='${HOME_PAGE}'; require('fs').writeFileSync('package.json', JSON.stringify(pkg, null, 2));\""
  },
